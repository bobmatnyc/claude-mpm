{
  "mcpServers": {
    "mcp-vector-search": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-vector-search",
        "mcp"
      ],
      "env": {
        "MCP_ENABLE_FILE_WATCHING": "true"
      }
    },
    "mcp-ticketer": {
      "type": "stdio",
      "command": "/Users/masa/.local/pipx/venvs/mcp-ticketer/bin/mcp-ticketer",
      "args": [
        "mcp"
      ]
    },
    "mcp-browser": {
      "type": "stdio",
      "command": "/Users/masa/.local/pipx/venvs/mcp-browser/bin/python",
      "args": [
        "-m",
        "mcp_browser.cli.main",
        "mcp"
      ],
      "env": {
        "MCP_BROWSER_HOME": "/Users/masa/.mcp-browser"
      }
    }
  }
}